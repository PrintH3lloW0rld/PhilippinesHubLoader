local notification = Instance.new("Sound") 
notification.Parent = game:GetService("SoundService")
notification.SoundId = "rbxassetid://9086208751"
notification.Volume = 20

game.StarterGui:SetCore("SendNotification", {
      Icon = "http://www.roblox.com/asset/?id=10074877413";
      Title = "Philippines Hub: Loader", 
      Text = "Checking Game🔃 🎮...";
notification:play()
})
wait(5)

game.StarterGui:SetCore("SendNotification", {
    Icon = "http://www.roblox.com/asset/?id=10074877413";
    Title = "Philippines Hub: Loader", 
    Text = "Game 🎮:✅ Blox Fruits 🍇";
notification:play()
})
wait(1)

game.StarterGui:SetCore("SendNotification", {
    Icon = "http://www.roblox.com/asset/?id=10074877413";
    Title = "Philippines Hub v2.0", 
    Text = "Script Loading🔃...";
notification:play()
})
wait(1)

game.StarterGui:SetCore("SendNotification", {
    Icon = "http://www.roblox.com/asset/?id=10074877413";
    Title = "Philippines Hub v2.0", 
    Text = "Loaded 🎉... Enjoy 👍";
notification:play()
})
wait(.5)

loadstring(game:HttpGet('https://raw.githubusercontent.com/PrintH3lloW0rld/PhilippinesHubLoader/main/BFv3'))()
wait(.5)

game.StarterGui:SetCore("SendNotification", {
  Icon = "";
    Title = "Philippines Hub: Loader", 
    Text = "Welcome, thx for using my script😊👌"..game.Players.LocalPlayer.Name.."!!";
})
